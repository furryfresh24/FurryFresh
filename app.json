{
  "expo": {
    "name": "furryfresh",
    "slug": "furryfresh",
    "scheme": "furryfresh",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/furry-fresh-logo.png",
      "resizeMode": "contain",
      "backgroundColor": "#F8F8FF"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/furry-fresh-logo.png",
        "backgroundColor": "#F8F8FF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ]
  }
}
